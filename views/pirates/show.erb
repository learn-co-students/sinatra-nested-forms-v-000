<h1>Pirate: </h1>
 <div>
  <h2>Name: <%= @pirate.name %></h2>
  <h3>Weight: <%= @pirate.weight %></h3>
  <h3>Height: <%= @pirate.height %></h3>
 </div>

<h1>Ships: </h1>

<div>
  <% @ships.map do |ship| %>
    <h2>Name: <%= ship.name %></h2>
    <h3>Type: <%= ship.type %></h3>
    <h3>Booty: <%= ship.booty %></h3>
  <% end %>
</div>
