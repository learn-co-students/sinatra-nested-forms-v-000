<h1>Display your Pirate here</h1>
Name: <%= @pirate.name %><br>
Weight: <%= @pirate.weight %><br>
Height: <%= @pirate.height %><br>
<h2>Display your ships here</h2>
<% @ships.each.with_index(1) do |ship, i| %>
  <%= "Ship #{i} Name: #{ship.name}"%><br>
  <%= "Ship #{i} Type: #{ship.type}"%><br>
  <%=  "Ship #{i} Booty: #{ship.booty}"%><br><br>
<% end %>
