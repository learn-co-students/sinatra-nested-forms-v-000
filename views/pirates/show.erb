<h1>Display your Pirate here</h1>
<% @pirate.attributes.each do |key, val| %>
	<p><%= key %>: <%= val %></p>
<% end %>
<h2>Display your first ship here</h2>
<% @ships[0].attributes.each do |key, val| %>
	<p><%= key %>: <%= val %></p>
<% end %>

<h2>Display your second ship here</h2>
<% @ships[1].attributes.each do |key, val| %>
	<p><%= key %>: <%= val %></p>
<% end %>