<body>
  <h1>Information on your pirate</h1>
  <h2>Name:<%= @pirate.name%><h2><br>
  <h2>Weight:<%= @pirate.weight%><h2><br>
  Height: <%= @pirate.height %><h2><br>


  <% @ships.each do |ship| %>
  <h2>Ship Name:  <%= ship.name %><h2><br>
  <h2>Ship Type: <%= ship.type %><h2><br>
  <h2>Ship Booty: <%= ship.booty %><h2><br>
  <% end %>
</body>
