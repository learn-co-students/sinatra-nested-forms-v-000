<div class="pirate">
  <h1> Name: <%= @pirate.name %> </h1>
  <h1> Height: <%= @pirate.height %> </h1>
  <h1> Weight: <%= @pirate.weight %> </h1>
</div
<br>


<% @ships.each do |ship| %>
  <div class="ship">
    <h2> Name: <%= ship.name %> </h2>
    <h2> Type: <%= ship.type %> </h2>
    <h2> Booty: <%= ship.booty %> </h2>
    <br>
    <% end %>
</div>
