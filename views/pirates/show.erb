<h1>Display your Pirate here</h1>
<div>
    <h4>Name: <%=@pirate.name%></h4>
    <h6>Weight: <%=@pirate.weight%></h6>
    <h6>Height: <%=@pirate.height%></h6>
</div>

<h2>Display your first ship here</h2>
<% @ships.each do |ship| %>
  <div>
    <h4>Name: <%=ship.name%></h4>
    <h6>Type: <%=ship.type%></h6>
    <h6>Booty: <%=ship.booty%></h6>
  </div>
<% end %>

