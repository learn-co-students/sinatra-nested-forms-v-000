<h1>Make your form here</h1>
<form action="/pirates" method="POST">
  <!--Pirate input for name, weight and height-->
  <label for="name">Pirate Name:</label>
  <input type="text" name="pirate[name]" id="name"><br>
  <label for="weight">Pirate Weight:</label>
  <input type="text" name="pirate[weight]" id="weight"><br>
  <label for="height">Pirate Height:</label>
  <input type="text" name="pirate[height]" id="height"><br>
  <!--Rendering Ships: name, type, booty-->
  <label for="name">Ship Name:</label>
  <input id="ship_name_1" type="text" name="pirate[ships][][name]"><input id="ship_name_2" type="text" name="pirate[ships][][name]"><br>

  <label for="type">Ship Type:</label>
  <input id="ship_type_1" type="text" name="pirate[ships][][type]"><input id="ship_type_2" type="text" name="pirate[ships][][type]"><br>

  <label for="booty">Ship Booty:</label>
  <input  id="ship_booty_1" type="text" name="pirate[ships][][booty]"><input id="ship_booty_2" type="text" name="pirate[ships][][booty]"><br>

  <input type="submit" name="submit" value="Submit">

</form>
