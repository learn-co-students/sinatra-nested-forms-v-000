	<p>Pirate Height: <input type="text" name="pirate[height]" id="pirate[height]"></p>
	<p>First Pirate Ship Name: <input type="text" name="pirate[ships][][name]" id="ship_name_1"></p>
	<p>First Pirate Ship Type: <input type="text" name="pirate[ships][][type]" id="ship_type_1"></p>
