<body>
  <h1>Make your form here</h1>
    <form action="/pirates" method="POST">
      <label for="pirate_name">Pirate Name: </label>
      <input id="pirate_name" input type="text" name="pirate[name]">
      
      <label for="pirate_weight">Pirate Weight: </label>
      <input id="pirate_weight" input type="text" name="pirate[weight]">
      
      <label for="pirate_height">Pirate Height: </label>
      <input id="pirate_height" input type="text" name="pirate[height]">

      <label for="ship_name_1">Ship Name: </label>
      <input id="ship_name_1" input type="text" name="pirate[ships][][name]">
      
      <label for="ship_type_1">Ship Type: </label>
      <input id="ship_type_1" input type="text" name="pirate[ships][][type]">
      
      <label for="ship_booty_1">Ship Booty: </label>
      <input id="ship_booty_1" input type="text" name="pirate[ships][][booty]">
      
      <label for="ship_name_2">Ship Name: </label>
      <input id="ship_name_2" input type="text" name="pirate[ships][][name]">
      
      <label for="ship_type_2">Ship Type: </label>
      <input id="ship_type_2" input type="text" name="pirate[ships][][type]">
      
      <label for="ship_booty_2">Ship Booty: </label>
      <input id="ship_booty_2" input type="text" name="pirate[ships][][booty]">
      
      <input type="submit" name="Submit" id="Submit">
    </form>
  </body>
</html>

