<form action="/pirates" method="post">
  Pirate Name: <input type="text" name="pirate[name]" id="pirate_name">
  Pirate Weight: <input type="text" name="pirate[weight]" id="pirate_weight">
  Pirate Height: <input type="text" name="pirate[height]" id="pirate_height">
  Ship 1 Name: <input type="text" name="pirate[ships][][name]" id="ship_name_1">
  Ship 1 Type: <input type="text" name="pirate[ships][][type]" id="ship_type_1">
  Ship 1 Booty: <input type="text" name="pirate[ships][][booty]" id="ship_booty_1">
  Ship 2 Name: <input type="text" name="pirate[ships][][name]" id="ship_name_2">
  Ship 2 Type: <input type="text" name="pirate[ships][][type]" id="ship_type_2">
  Ship 2 Booty: <input type="text" name="pirate[ships][][booty]" id="ship_booty_2">

  <input type="submit" value="Submit"> <!-- value case sensitive for rspec -->
</form>
