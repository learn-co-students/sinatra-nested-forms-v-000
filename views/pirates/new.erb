<h1>Make your form here</h1>
<form method="POST" action="/pirates">
  <label for="pirate">Pirate: </label>
    <p>Name: <input type="text" name="pirate[name]"></p>
    <p>Height: <input type="text" name="pirate[height]"></p>
    <p>Weight: <input type="text" name="pirate[weight]"></p>
  <label for="ship">Ship 1: </label>
    <p>Name: <input type="text" id="ship_name_1" name="pirate[ships][][name]"></p>
    <p>Type: <input type="text" id="ship_type_1" name="pirate[ships][][type]"></p>
    <p>Booty: <input type="text" id="ship_booty_1" name="pirate[ships][][booty]"></p>
  <label for="ship">Ship 2: </label>
    <p>Name: <input type="text" id="ship_name_2" name="pirate[ships][][name]"></p>
    <p>Type: <input type="text" id="ship_type_2" name="pirate[ships][][type]"></p>
    <p>Booty: <input type="text" id="ship_booty_2" name="pirate[ships][][booty]"></p>
  <input id="submit" name="submit" type="submit">
</form>
