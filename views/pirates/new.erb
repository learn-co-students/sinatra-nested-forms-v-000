<h1>Welcome to the Nested Forms Lab!</h1>

<form method="post" action="/pirates">
<label for="pirate[name]">Pirate Name</label>
<input name="pirate[name]" type="text"><br>

<label for="pirate[weight]">Pirate Weight</label>
<input name="pirate[weight]" type="text"><br>

<label for="pirate[height]">Pirate Height</label>
<input name="pirate[height]" type="text"><br>

<label for="ship_name_1">Ship 1 Name</label>
<input id="ship_name_1" type="text" name="pirate[ships][][name]"><br>
<label for="ship_type_1">Ship 1 Type</label>
<input id="ship_type_1" type="text" name="pirate[ships][][type]"><br>
<label for="ship_booty_1">Ship 1 Booty</label>
<input id="ship_booty_1" type="text" name="pirate[ships][][booty]"><br>

<label for="ship_name_2">Ship 2 Name</label>
<input id="ship_name_2" type="text" name="pirate[ships][][name]"><br>
<label for="ship_type_2">Ship 2 Type</label>
<input id="ship_type_2" type="text" name="pirate[ships][][type]"><br>
<label for="ship_booty_2">Ship 2 Booty</label>
<input id="ship_booty_2" type="text" name="pirate[ships][][booty]"><br>
<input type="submit" value="Submit" id="submit">
</form>

