<h1>Pirate Sign Up Sheet</h1>
<form method="POST" action="/pirates">
  <label>Pirate Name: </label><input type="text" name="pirate[name]"><br>
  <label>Pirate Weight: </label><input type="text" name="pirate[weight]"><br>
  <label>Pirate Height: </label><input type="text" name="pirate[height]"><br>
  <label>First Ship Name: </label><input type="text" name="pirate[ships][][name]" id="ship_name_1"><br>
  <label>First Ship Type: </label><input type="text" name="pirate[ships][][type]" id="ship_type_1"><br>
  <label>First Ship Booty: </label><input type="text" name="pirate[ships][][booty]" id="ship_booty_1"><br>
  <label>Second Ship Name: </label><input type="text" name="pirate[ships][][name]" id="ship_name_2"><br>
  <label>Second Ship Type: </label><input type="text" name="pirate[ships][][type]" id="ship_type_2"><br>
  <label>Second Ship Booty: </label><input type="text" name="pirate[ships][][booty]" id="ship_booty_2"><br>
  <input type="submit" value="Submit">
</form>
