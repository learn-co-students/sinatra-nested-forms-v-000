<h1>Welcome to the Nested Forms Lab!</h1>

  <form action="/pirates" method="post">
    <label for="pirate">Pirate: </label>
    <input type="text" name="pirate[name]" id="pirate">
    <p><label for="pirate[weight]">Weight: </label>
    <input type="text" name="pirate[weight]" id="pirate[weight]"></p>
    <p><label for="pirate[height]">Height: </label>
    <input type="text" name="pirate[height]" id="pirate[height]"></p>

    <p><label for="ship_name_1">Ship 1 Name: </label>
    <input type="text" name="pirate[ships][][name]" id="ship_name_1"></p>
    <p><label for="ship_type_1">Ship 1 Type: </label>
    <input type="text" name="pirate[ships][][type]" id="ship_type_1"></p>
    <p><label for="ship_booty_1">Ship 1 Booty: </label>
    <input type="text" name="pirate[ships][][booty]" id="ship_booty_1"></p>

    <p><label for="ship_name_2">Ship 2 Name: </label>
    <input type="text" name="pirate[ships][][name]" id="ship_name_2"></p>
    <p><label for="ship_type_2">Ship 2 Type: </label>
    <input type="text" name="pirate[ships][][type]" id="ship_type_2"></p>
    <p><label for="ship_booty_2">Ship 2 Booty: </label>
    <input type="text" name="pirate[ships][][booty]" id="ship_booty_2"></p>

    <input type="submit" name="Submit" value="Submit">
  </form>
