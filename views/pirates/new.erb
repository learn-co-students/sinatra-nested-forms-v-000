<h1>Make your form here</h1>
<form action="/pirates" method="post">
  <label for="name">Pirate Name:</label>
  <input type="text" name="pirate[name]"> <br>

  <label for="weight">Pirate Weight:</label>
  <input type="text" name="pirate[weight]"> <br>

  <label for="height">Pirate Height:</label>
  <input type="text" name="pirate[height]"> <br>

  <label for="ship name">Ship1 Name:</label>
  <input id="ship_name_1" type="text" name="pirate[ships][][name]"> <br>

  <label for="ship type">Ship1 Type:</label>
  <input id="ship_type_1" type="text" name="pirate[ships][][type]"> <br>

  <label for="ship booty">Ship1 Booty:</label>
  <input id="ship_booty_1"  type="text" name="pirate[ships][][booty]"> <br>

  <label for="ship name">Ship2 Name:</label>
  <input id="ship_name_2" type="text" name="pirate[ships][][name]"> <br>

  <label for="ship type">Ship2 Type:</label>
  <input id="ship_type_2" type="text" name="pirate[ships][][type]"> <br>

  <label for="ship booty">Ship2 Booty:</label>
  <input id="ship_booty_2" type="text" name="pirate[ships][][booty]">

  <input type="submit" value="submit">
</form>
