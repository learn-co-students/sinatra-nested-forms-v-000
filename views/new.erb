<h1>Make your form here</h1>

<form method="POST" action="/pirates">
  Pirate Name: <input type="text" name="pirate[name]">
  Pirate Weight: <input type="text" name="pirate[weight]">
  Pirate Height: <input type="text" name="pirate[height]">
  First Ship Name: <input type="text" name="pirate[ships][][name]" id="ship_name_1">
  First Ship Type: <input type="text" name="pirate[ships][][type]" id="ship_type_1">
  First Ship Booty: <input type="text" name="pirate[ships][][booty]" id="ship_booty_1">
  Second Ship Name: <input type="text" name="pirate[ships][][name]" id="ship_name_2">
  Second Ship Type: <input type="text" name="pirate[ships][][type]" id="ship_type_2">
  Second Ship Booty: <input type="text" name="pirate[ships][][booty]]" id="ship_booty_2">
  <button type='submit' value='submit'>Submit</button>
</form>
