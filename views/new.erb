<h1>Make your form here</h1>

<h1>Create a Pirate and a Ship</h1>
<form method="POST" action="/pirates">
  <!-- FormsLab::App GET '/new' renders the pirate input fields for name, weight, and height attributes on the page -->
  <p>Pirate Name:<input type="text" name="pirate[name]"></p>
  <p>Pirate Weight:<input type="text" name="pirate[weight]"></p>
  <p>Pirate Height:<input type="text" name="pirate[height]"></p>
  <!-- FormsLab::App GET '/new' renders the first ship's input fields for the name, type, and booty attributes on the page -->
  <p>Ship Name:<input id="ship_name_1" type="text" name="pirate[ships][][name]"></p>
  <p>Ship Type:<input id="ship_type_1" type="text" name="pirate[ships][][type]"></p>
  <p>Ship Booty:<input id="ship_booty_1" type="text" name="pirate[ships][][booty]"></p>

  <p>Second Ship Name:<input id="ship_name_2" type="text" name="pirate[ships][][name]"></p>
  <p>Second Ship Type:<input id="ship_type_2" type="text" name="pirate[ships][][type]"></p>
  <p>Second Ship Booty:<input id="ship_booty_2" type="text" name="pirate[ships][][booty]"></p>

  <input type="submit" title='Submit'>
</form>